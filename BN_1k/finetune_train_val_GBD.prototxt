name: "BN-Inception"

####################################### data #######################################
layer {
  name: "data"
  type: "FrcnnTrainData"
  top: "data"
  top: "labels_single"
  top: "rois"
  top: "bbox_targets_single"
  top: "bbox_weights_single"
  frcnn_train_data_param {
    source: "../rois/rois_file_trainval.txt"
    source: "../rois/rois_file_pos1k.txt"
    root_folder: ""
    root_folder: ""
    batch_size: 32
    batch_size: 8
    ims_per_batch: 1
    gt_per_batch: 2
    scales: 600
    max_size: 700
    fg_thresh: 0.5
    fg_thresh: 0.7
    bg_thresh_hi: 0.5
    bg_thresh_lo: 0.0
    fg_fraction: 0.25
    num_class: 201
    rand_seed: 3
    rand_skip: 0
  }
  transform_param {
    mean_value: 103.939
    mean_value: 116.779
    mean_value: 123.68
  }
  include: { phase: TRAIN }
}
layer {
  name: "Gather"
  type: "Gather"
  bottom: "labels_single"
  top: "labels"
}
layer {
  name: "Gather"
  type: "Gather"
  bottom: "bbox_targets_single"
  top: "bbox_targets"
}
layer {
  name: "Gather"
